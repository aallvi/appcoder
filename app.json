{
  "expo": {
    "name": "Applanetary",
    "slug": "Applanetary",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/planeta-tierra.png",
    "notification":{
      "icon":"./assets/space.png"
   },
    "splash": {
      "image": "./assets/porta.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier":"com.univers.alvi",
      "buildNumber":"1.0.0",
      "supportsTablet": true
    },
    "android": {
      "package":"com.univers.alvi",
      "versionCode":1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/space.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions":[
        "ACCESS_FINE_LOCATION",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
